{
  "name": "paygator-webhook-api",
  "version": "1.0.0",
  "description": "API REST para integração de webhooks de provedores de pagamento",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && node scripts/copy-views.js",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --require tsconfig-paths/register src/index.ts",
    "dev:all": "powershell -ExecutionPolicy Bypass -File start-dev.ps1",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t paygator-webhook-api .",
    "docker:run": "docker run -p 3000:3000 paygator-webhook-api",
    "generate-keys": "node scripts/generate-keys.js",
    "keys": "npm run generate-keys",
    "test-db": "node scripts/test-database-connection.js",
    "setup-local-db": "node scripts/setup-local-postgres.js",
    "setup-docker": "node scripts/setup-local-docker.js",
    "start-docker": "powershell -ExecutionPolicy Bypass -File scripts/start-docker.ps1",
    "migrate-to-render": "node scripts/migrate-to-render-postgres.js",
    "test-migration": "node scripts/migrate-to-render-postgres.js test",
    "render-diagnostic": "node scripts/render-diagnostic.js",
    "setup-sqlite": "node scripts/setup-sqlite.js",
    "test-sqlite": "npm run build && node scripts/setup-sqlite.js",
    "clear-db": "node scripts/clear-database.js",
    "clear-sqlite": "node scripts/clear-sqlite.js",
    "clear-all": "node scripts/clear-all-databases.js"
  },
  "keywords": [
    "webhook",
    "payment",
    "api",
    "rest",
    "typescript"
  ],
  "author": "Paygator Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.52.0",
    "@types/body-parser": "^1.19.6",
    "@types/ejs": "^3.1.5",
    "@types/pg": "^8.15.4",
    "@types/sqlite3": "^3.1.11",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "mongoose": "^8.0.3",
    "pg": "^8.16.3",
    "postgres": "^3.4.8",
    "redis": "^4.6.10",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
